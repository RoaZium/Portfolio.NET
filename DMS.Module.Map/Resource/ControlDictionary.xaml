    <!--공정 구성 알람 상태 표시-->
    <Style x:Key="mRoutingConfiguBorder" TargetType="{x:Type Border}" BasedOn="{StaticResource xBorderBase}">
        <Style.Triggers>
            <DataTrigger Binding="{Binding DmComponentM.PropertyM.BlinkingLevel}" Value="{x:Null}">
                <DataTrigger.EnterActions>
                    <BeginStoryboard Name="wave_storyboard">
                        <Storyboard Storyboard.TargetProperty="Width">
                            <DoubleAnimation From="0" To="1" Duration="0:0:2" RepeatBehavior="Forever"/>
                        </Storyboard>
                    </BeginStoryboard>
                </DataTrigger.EnterActions>
                <DataTrigger.ExitActions>
                    <StopStoryboard BeginStoryboardName="wave_storyboard"/>
                </DataTrigger.ExitActions>
            </DataTrigger>
            <DataTrigger Binding="{Binding DmComponentM.PropertyM.BlinkingLevel}" Value="0">
                <DataTrigger.EnterActions>
                    <BeginStoryboard Name="wave_storyboard0">
                        <Storyboard Storyboard.TargetProperty="Width">
                            <DoubleAnimation From="0" To="1" Duration="0:0:2" RepeatBehavior="Forever"/>
                        </Storyboard>
                    </BeginStoryboard>
                </DataTrigger.EnterActions>
                <DataTrigger.ExitActions>
                    <StopStoryboard BeginStoryboardName="wave_storyboard0"/>
                </DataTrigger.ExitActions>
            </DataTrigger>
            <DataTrigger Binding="{Binding DmComponentM.PropertyM.BlinkingLevel}" Value="1">
                <DataTrigger.EnterActions>
                    <BeginStoryboard Name="wave_storyboard1">
                        <Storyboard Storyboard.TargetProperty="Width">
                            <DoubleAnimation From="0" To="1" Duration="0:0:0.5" RepeatBehavior="Forever"/>
                        </Storyboard>
                    </BeginStoryboard>
                </DataTrigger.EnterActions>
                <DataTrigger.ExitActions>
                    <StopStoryboard BeginStoryboardName="wave_storyboard1"/>
                </DataTrigger.ExitActions>
            </DataTrigger>
        </Style.Triggers>
    </Style>
    
    <!--공정 구성 알람 상태 Grid-->
    <Style x:Key="mRoutingConfigGrid" TargetType="{x:Type Grid}" BasedOn="{StaticResource sBaseGrid}">
        <Setter Property="Height" Value="{Binding Width, RelativeSource={RelativeSource Self}, Mode=OneWay}"/>
        <Setter Property="Visibility" Value="Hidden"/>
        <Setter Property="Width">
            <Setter.Value>
                <MultiBinding Converter="{StaticResource multiMinConverter}">
                    <Binding Path="ActualWidth" ElementName="root"/>
                    <Binding Path="ActualHeight" ElementName="root"/>
                </MultiBinding>
            </Setter.Value>
        </Setter>
    </Style>
    
    <!--공정 구성 알람 상태 Ellipse-->
    <Style x:Key="mRoutingConfigEllipse" TargetType="{x:Type Ellipse}" BasedOn="{StaticResource xEllipseBase}">
        <Setter Property="Fill" Value="{Binding DmComponentM.PropertyM.AlarmState, Converter={StaticResource xAlarmStateColorConverter}}"/>
        <Setter Property="Height">
            <Setter.Value>
                <MultiBinding Converter="{StaticResource multiMultiplyConverter}">
                    <Binding Path="ActualHeight" ElementName="squre_container"/>
                    <Binding Path="Width" ElementName="animation_value"/>
                </MultiBinding>
            </Setter.Value>
        </Setter>
            <Setter Property="Opacity" Value="{Binding ElementName=animation_value, Path=Width, Converter={StaticResource singleRangeConverter}}"/>
        <Setter Property="Width">
            <Setter.Value>
                <MultiBinding Converter="{StaticResource multiMultiplyConverter}">
                    <Binding Path="ActualWidth" ElementName="squre_container"/>
                    <Binding Path="Width" ElementName="animation_value"/>
                </MultiBinding>
            </Setter.Value>
        </Setter>
    </Style>

