                    <x:Null/>
                </ObjectDataProvider.ConstructorParameters>
            </ObjectDataProvider>
        </DataTemplate.Resources>
        <ContentPresenter Content="{Binding Source={StaticResource content}}"/>
    </DataTemplate>
    <!-- 모니터링 시나리오 끝 -->

    <!-- 알람 모니터링 시나리오 시작 -->
